                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Declare @mesAnterior int,@fechaAnterior varchar(6)
set @mesAnterior=month( GetDate())-1

select @fechaAnterior= 
concat( 
		convert(varchar, case  when @mesAnterior = 0 then YEAR(GetDate())-1 else YEAR(GetDate()) end  )
		,
		case when @mesAnterior = 0 then '12' else ( case when @mesAnterior&lt;10 then ('0'+convert(varchar, @mesAnterior)) else CONVERT(varchar,  @mesAnterior) end )
end)

